{
  "name": "blockport",
  "version": "0.1.0",
  "author": "Julie Ng",
  "description": "A dead simple and developer friendly JavaScript library for handline object storage on Azure",
  "keywords": [
    "azure",
    "blob",
    "storage",
    "blob-storage",
    "uploads"
  ],
  "main": "source/blockport.js",
  "private": true,
  "scripts": {
    "test": "jest",
    "test:watch": "nodemon -w ./source -w ./tests --exec 'npm run test'",
    "jsdoc": "jsdoc --readme README.md -c jsdoc.json",
    "jsdoc:dev": "nodemon -w ./source --exec 'npm run jsdoc -- --private'"
  },
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.0",
    "fast-xml-parser": "^3.15.1"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "jest": "^24.9.0",
    "jsdoc": "^3.6.3",
    "nodemon": "^2.0.2",
    "tidy-jsdoc": "^1.1.0"
  }
}
